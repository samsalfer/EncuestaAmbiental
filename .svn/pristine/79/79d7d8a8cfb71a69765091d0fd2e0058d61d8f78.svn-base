<script>
function validate5() {

	res = concordancia1();

	resTotal = res;
	return resTotal;
}

function concordancia1() {
	a311 = document.getElementById("a311");
	a312 = document.getElementById("a312");
	
	a314 = document.getElementById("a314");
	a315 = document.getElementById("a315");
	
	eOtros = document.getElementById("eOtros");
	res = true;
	
	//Hombres
	if (a311.value != 0) {
		a314.required = true;
		a314.min = "1";
	} else {
		a314.required = false;
		a314.min = "0";
	}

	if (a314.value != 0) {
		a311.required = true;
		a311.min = "1";
	} else {
		a311.required = false;
		a311.min = "0";
	}

	//Mujeres
	if (a312.value != 0) {
		a315.required = true;
		a315.min = "1";
	} else {
		a315.required = false;
		a315.min = "0";
	}

	if (a315.value != 0) {
		a312.required = true;
		a312.min = "1";
	} else {
		a312.required = false;
		a312.min = "0";
	}

	
	//ponemos a 0 todos los atributos para que no falle al hacer las sumas.
	
	var frm = document.getElementById("commentForm");
	for (i=0;i<frm.elements.length;i++)
	{
		if(frm.elements[i].value.length==0){
			frm.elements[i].value=0;
		}
	}
	
	if(eOtros.value==0){
		eOtros.value="";
	}
	
	return res;
}




</script>
